{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "jackmoore/colorbox",
                "version": "1.6.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3",
        "drupal/block_content_permissions": "^1.6",
        "drupal/bootstrap": "^3",
        "drupal/captcha": "^2.0",
        "drupal/chosen": "^5.0",
        "drupal/config_ignore": "^3",
        "drupal/config_split": "^2.0",
        "drupal/contact_block": "^2.0",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/ctools": "^4.1",
        "drupal/devel": "^5.0",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/ds": "^3.1",
        "drupal/entity_browser": "~2.0",
        "drupal/field_group": "^3.0",
        "drupal/field_group_link": "^3",
        "drupal/field_image_link": "1.x-dev",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "^2.2",
        "drupal/hook_post_action": "^1.2",
        "drupal/icon_select": "^3.0",
        "drupal/image_widget_crop": "^3.0",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/paragraphs": "^1",
        "drupal/pathauto": "^1.6",
        "drupal/responsive_table_filter": "^2.0",
        "drupal/responsive_tables_filter": "^2.0",
        "drupal/shield": "^1.2",
        "drupal/social_media_links": "^2.6",
        "drupal/structure_sync": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/video_embed_field": "^3.0@beta",
        "drupal/views_bootstrap": "^3.1",
        "drupal/views_parity_row": "^1.8",
        "drush/drush": "^13",
        "jackmoore/colorbox": "^1.6",
        "jigarius/drall": "^4.0",
        "mglaman/composer-drupal-lenient": "^1.0",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "mglaman/composer-drupal-lenient": true,
            "mnsami/composer-custom-directory-installer": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
                "web-root": "web/"
            }
        },
        "enable-patching": true,
        "patches": {
            "drupal/views_parity_row": {
                "3384799: Call to undefined method preRenderByRelationship": "https://www.drupal.org/files/issues/2023-10-10/3384799-8.undefined-relationship-fix.patch"
            }
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/field_image_link"]
        }
    }
}
